<style>
  .namedcolours__header {
    padding-bottom: 3rem;
  }

  .namedcolours {
    display: block;
    column-width: 300px;
    list-style: none;
    gap: 30px;
    padding: 0;
    margin: 0;
  }

  .namedcolours__item {
    border-radius: 10px;
    animation: fadeIn 0.3s forwards;
    border: 3px solid var(--text);
    padding: 0.6rem 1rem;
    margin: 0 0 30px 0;
    break-inside: avoid;
  }

  .namedcolours__item.--black {
    color: black;
  }

  .namedcolours__item.--white {
    color: white;
  }

  .namedcolours__item.--black .blob__white,
  .namedcolours__item.--black .blob__black {
    border-color: black;
  }

  .namedcolours__item.--white .blob__white,
  .namedcolours__item.--white .blob__black {
    border-color: white;
  }

  .namedcolours__none {
    display: none;
  }

  .namedcolours__cc {
    display: grid;
    grid-template-columns: 1fr 1fr;
    place-items: center;
  }

  .namedcolours__cc div {
    display: flex;
    align-items: center;
    gap: 10px;
  }

  .blob {
    width: 40px;
    height: 40px;
    border-radius: 30px;
    border: 3px solid;
    display: block;
    font-size: 0 !important;
  }

  .blob__white {
    background-color: white;
  }

  .blob__black {
    background-color: black;
  }


  dl {
    font-weight: 500;
    width: 100%;
    display: grid;
    grid-template-columns: 40% 60%;
    gap: 10px;
  }

  dl dt,
  dl dd {
    margin: 0;
  }

</style>

