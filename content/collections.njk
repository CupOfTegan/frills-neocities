---
title: Collections
rss: false
style: '
.dashboard__item {
  position: relative;
}

.dashboard__item header a {
  position: static;
}

.dashboard__item header a:hover,
.dashboard__item header a:focus,
.dashboard__item header a:active {
  color: var(--text)
}

.dashboard__item header a::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
'
---
{% extends "structure/pages/list-collections.njk" %}

{% block main %}
<section class="dashboard">
  <article class="dashboard__item --card">
    <figure class="dashboard__item-image --square">
      <img src="/images/collections/stickers/cover.png" alt="" loading="lazy" role="presentation">
    </figure>
    <header>
        <h2>
          <a href="/collections/sticker-album" class="hidden">Sticker album</a>
        </h2>
      </header>
  </article>
  <article class="dashboard__item --card">
    <figure class="dashboard__item-image --square">
      <img src="/images/collections/avatars/cover.png" alt="" loading="lazy" role="presentation">
    </figure>
    <header>
        <h2>
          <a href="/collections/avatars" class="hidden">Avatars</a>
        </h2>
      </header>
  </article>
</section>
{% endblock %}