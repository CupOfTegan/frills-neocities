<section class="reading-width">
  <ul class="buttonwall">
    {% for link in links %}
    {% if link.type == "button" %}
    <li class="buttonwall__image">
      <a href="{{button.href}}">
          <img src="/images/buttons/{{button.src}}" alt="{{button.alt}}">
      </a>
    </li>
    {% else %}
    <li class="buttonwall__item">
      <a href="{{link.href}}" style="font-size: {{link.size}};  background-color: {{link.bg}}; color: {{link.color}};">
        {{link.name}}
      </a>
    </li>
    {% endfor %}
  </ul>

  <h2>Friends</h2>
  <ul class="buttonwall">
    {% for friend in friends %}
    <li class="buttonwall__item">
      <a href="{{friend.href}}" style="font-size: {{friend.size}};  background-color: {{friend.bg}}; color: {{friend.color}};">
        {{friend.name}}
      </a>
    </li>
    {% endfor %}
  </ul>

  <h2>Creatives</h2>
  <ul class="buttonwall">
    {% for creative in creatives %}
    <li class="buttonwall__item">
      <a href="{{creative.href}}" style="font-size: {{creative.size}};  background-color: {{creative.bg}}; color: {{creative.color}};">
        {{creative.name}}
      </a>
    </li>
    {% endfor %}
  </ul>

  <h2>Tools & fun things</h2>
  <ul class="buttonwall">
    <li class="buttonwall__image">
      <a href="https://wiby.me/">
        <img src="/images/buttons/wiby.png" alt="Wiby">
      </a>
    </li>
